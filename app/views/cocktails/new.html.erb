
<div class="container">
  <%= simple_form_for [@cocktail] do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
     <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "add a ingredient", class: "btn btn-primary" %>
  <% end %>

  <%= link_to "back to the list", cocktails_path %>
</div>


